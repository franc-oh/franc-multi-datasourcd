<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.franc.dao.postgres.TestDao">

    <select id="selectEmpById" parameterType="Long" resultType="com.franc.vo.postgres.EmpVO">
        /** TestMapper.selectEmpById */
        select a.*
        from test.emp a
        where a.empno = #{empno}
    </select>

    <select id="selectEmpList" resultType="com.franc.vo.postgres.EmpVO">
        /** TestMapper.selectEmpList */
        select a.*
        from test.emp a
    </select>

</mapper>